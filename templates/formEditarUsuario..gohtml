{{ define "Editar"}}
    {{ template "Head"}}
        <div class="row">
            <a href="/"
            ><button
                type="button"
                class="btn btn-primary"
                style="margin-bottom: 20px; margin-right: 10px"
            >
                inicio
            </button></a
            >
        </div>
        <div class="row">
            <h5 style="padding: 10px">Editar Usu√°rio</h5>
        </div>
        <div class="row">
            <div class="col-md-6">
            <form
                action="/usuario/editado/"
                method="POST"
                style="margin-bottom: 20px"
            >
                <input
                type="hidden"
                class="form-control"
                name="id"
                value="{{ .ID}}"
                />
                <input
                type="text"
                class="form-control"
                name="nome"
                value="{{ .Nome}}"
                placeholder="Nome"
                />
                <input
                type="text"
                class="form-control"
                name="sobrenome"
                value="{{ .Sobrenome}}"
                placeholder="Sobrenome"
                />
                <input
                type="email"
                class="form-control"
                name="email"
                value="{{ .Email}}"
                placeholder="E-mail"
                />
                <input
                type="text"
                class="form-control"
                name="perfil"
                value="{{ .Perfil}}"
                placeholder="Perfil"
                />
                <input
                type="text"
                class="form-control"
                name="mandato"
                value="{{ .Mandato}}"
                placeholder="Mandato"
                />
                <input
                type="text"
                class="form-control"
                name="foto"
                value="{{ .Foto}}"
                placeholder="Foto"
                />
                <input
                type="text"
                class="form-control"
                name="naturalidade"
                value="{{ .Naturalidade}}"
                placeholder="Naturalidade"
                style="margin-bottom: 20px"
                />
                <button type="submit" class="btn btn-primary">Enviar</button>
            </form>
            </div>
        </div>
        </div>
    {{ template "Foot" }}
{{ end }}
