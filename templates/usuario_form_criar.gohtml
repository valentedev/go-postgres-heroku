{{ define "Novo"}}
    {{ template "Head"}}
        <div class="row">
        <a href="/usuarios/"
          ><button
            type="button"
            class="btn btn-primary"
            style="margin-bottom: 20px; margin-right: 10px"
          >
            inicio
          </button></a
        >
      </div>
      <div class="row">
        <h5 style="padding: 10px">Criar Novo Usu√°rio</h5>
      </div>
      <div class="row">
        <div class="col-md-6">
          <form
            action="/usuario/criado/"
            method="POST"
            style="margin-bottom: 20px"
          >
            <input
              type="text"
              class="form-control"
              name="nome"
              placeholder="Nome"
            />
            <input
              type="text"
              class="form-control"
              name="sobrenome"
              placeholder="Sobrenome"
            />
            <input
              type="email"
              class="form-control"
              name="email"
              placeholder="E-mail"
            />
            <input
              type="password"
              class="form-control"
              name="senha"
              placeholder="Senha"
            />
            <select
              type="text"
              class="form-control"
              name="acesso"
              placeholder="Acesso"
            >
            <option value="admin">Admin</option>
            <option value="usuario">Usuario</option>
            </select>
            <button type="submit" class="btn btn-primary mt-3">Enviar</button>
          </form>
        </div>
      </div>
    </div>

    {{ template "Foot" }}
{{ end }}
